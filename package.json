{
  "name": "gatsby-opinionated-starter",
  "description": "A very opinionated TypeScript netlify-ready gatsby starter",
  "version": "0.4.1",
  "author": "Yaroslav Kasperovych <ilovewindows96@gmail.com>",
  "dependencies": {
    "@types/react-helmet": "^6.0.0",
    "@types/typography": "^0.16.3",
    "gatsby": "^2.23.12",
    "gatsby-image": "^2.4.9",
    "gatsby-plugin-manifest": "^2.4.14",
    "gatsby-plugin-offline": "^3.2.13",
    "gatsby-plugin-react-helmet": "^3.3.6",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^2.6.14",
    "gatsby-plugin-typography": "^2.5.10",
    "gatsby-source-filesystem": "^2.3.14",
    "gatsby-transformer-sharp": "^2.5.7",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-typography": "^0.16.19",
    "typography": "^0.16.19",
    "typography-theme-moraga": "^0.16.19"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.10.4",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.5",
    "@testing-library/user-event": "^12.0.11",
    "@types/jest": "^26.0.4",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-gatsby": "^0.5.2",
    "babel-preset-react-app": "^9.1.2",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "remark-cli": "^8.0.0",
    "remark-lint": "^7.0.1",
    "remark-preset-lint-recommended": "^4.0.1",
    "stylelint": "^13.6.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-scss": "^3.18.0",
    "typescript": "^3.9.6",
    "typescript-plugin-css-modules": "^2.4.0"
  },
  "keywords": [
    "gatsby",
    "gatsby starter",
    "typescript",
    "eslint",
    "stylelint",
    "scss",
    "css modules",
    "typography",
    "jest",
    "testing-library",
    "storybook",
    "netlify",
    "github actions",
    "remark-lint",
    "husky",
    "lint-staged",
    "renovate",
    "pwa"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest --coverage",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "remark . && stylelint **/*.scss && eslint ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/datacrafts-io/gatsby-opinionated-starter"
  },
  "bugs": {
    "url": "https://github.com/datacrafts-io/gatsby-opinionated-starter/issues"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run test",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "*.scss": [
      "stylelint"
    ],
    "*.{md,mdx}": [
      "remark"
    ]
  }
}
